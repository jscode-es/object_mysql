{
  "name": "object_mysql",
  "version": "1.0.2",
  "description": "Recuperar bases de datos en formato objecto de Javascript",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jscode-es/object_mysql.git"
  },
  "keywords": [
    "mysql",
    "promise",
    "object"
  ],
  "author": "( JSCode ) Sergio Gonz√°lez Garrido",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jscode-es/object_mysql/issues"
  },
  "homepage": "https://github.com/jscode-es/object_mysql#readme",
  "nodemonConfig": {
    "watch": [
      "src/**/*"
    ],
    "ext": "ts",
    "exec": "npm run build && node ./dist/index.js",
    "verbose": false,
    "legacyWatch": true
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/joi": "17.2.3",
    "@types/node": "17.0.24",
    "cross-env": "7.0.3",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "dotenv": "16.0.0",
    "joi": "17.6.0",
    "promise-mysql": "5.1.0"
  }
}